#
# system.prop for tf300
#
ro.carrier=wifi-only
ro.sf.lcd_density=160

ro.ethernet.interface=eth0
ro.ethernet.autoEnable=yes
sensor.loop.minms=0
orientation.magnitude.threshold=50
ro.sys.fw.bg_apps_limit = 12
ro.sys.fw.use_trim_settings=true
ro.sys.fw.trim_empty_percent=90
ro.sys.fw.trim_cache_percent=90
ro.sys.fw.bservice_enable=true

media.stagefright.legacyencoder=1
media.stagefright.less-secure=1

# don't preload OpenGL in Zygote, the Tegra drivers do not like it
ro.zygote.disable_gl_preload=true
